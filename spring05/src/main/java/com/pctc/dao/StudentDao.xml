<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.pctc.dao.StudentDao">

    <select id="selectStudents" resultType="com.pctc.domain.Student">
        select t_id as tId,t_name as tName,t_age as tAge from student order by t_id desc
    </select>

</mapper>